[run]
source = .
omit = 
    */venv/*
    */.venv/*
    */env/*
    */.env/*
    */tests/*
    */test_*
    setup.py
    conftest.py
    __pycache__/*
    */__pycache__/*
    .pytest_cache/*
    htmlcov/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod 